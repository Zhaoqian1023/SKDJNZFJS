<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.po">

	<class name="User" table="t_user">
	    <id name="uId" column="Uid" type="integer">
	    	<generator class="native"></generator>
	    </id>
		<property name="uName" column="Uname" type="string" length="20"></property>
		<property name="uPwd" column="Upwd" type="string" length="255" ></property>
		<property name="role" column="Role" type="integer" length="2" ></property>
	</class>
	
</hibernate-mapping>
